# Production Environment Variables

# API Configuration
REACT_APP_API_URL=https://api.your-domain.com/api
REACT_APP_API_TIMEOUT=30000

# Map Configuration
REACT_APP_MAP_TILE_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
REACT_APP_MAP_ATTRIBUTION='&copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors'
REACT_APP_DEFAULT_MAP_CENTER=[39.8283, -98.5795]
REACT_APP_DEFAULT_MAP_ZOOM=4

# Application Settings
REACT_APP_NAME="FMCSA ELD Log Generator"
REACT_APP_DESCRIPTION="Generate FMCSA-compliant Electronic Logging Device records"
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_DATE=2024-01-01

# Security
HTTPS=true
PUBLIC_URL=https://your-domain.com

# Performance
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false

# Analytics
REACT_APP_GA_TRACKING_ID=G-XXXXXXXXXX
REACT_APP_SENTRY_DSN=https://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx@sentry.io/xxxxxxx

# Error Tracking
REACT_APP_LOG_LEVEL=error

# PWA Configuration
REACT_APP_PWA_ENABLED=true
REACT_APP_OFFLINE_ENABLED=true

# Feature Flags
REACT_APP_ENABLE_MAPS=true
REACT_APP_ENABLE_CHARTS=true
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_SAVE=true
REACT_APP_ENABLE_ANALYTICS=true